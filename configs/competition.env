# VuenCode Competition Configuration
# Optimized for maximum performance and sub-500ms latency

# === Deployment Settings ===
VUENCODE_DEPLOYMENT_MODE=competition
VUENCODE_PERFORMANCE_PROFILE=competition

# === API Configuration ===
VUENCODE_API_HOST=0.0.0.0
VUENCODE_API_PORT=8000
VUENCODE_API_WORKERS=4

# === Model Configuration ===
# Set your Gemini API key here for production
VUENCODE_GEMINI_API_KEY=AIzaSyA4LCrRRlopK-lFxPNBq6WSMFQ3_PacDc8
VUENCODE_GEMINI_FLASH_MODEL=gemini-2.5-flash
VUENCODE_GEMINI_PRO_MODEL=gemini-2.5-pro

# Model selection optimization
VUENCODE_COMPLEXITY_THRESHOLD=0.7
VUENCODE_MAX_RETRIES=2
VUENCODE_REQUEST_TIMEOUT=25

# === Performance Settings (Competition Optimized) ===
VUENCODE_TARGET_LATENCY_MS=400
VUENCODE_MAX_CONCURRENT_REQUESTS=20

# === Video Processing (High Performance) ===
VUENCODE_MAX_FRAMES_PER_VIDEO=64
VUENCODE_DEFAULT_FPS_SAMPLE=2.0
VUENCODE_SCENE_DETECTION_THRESHOLD=0.25

# === GPU Acceleration (Enabled) ===
VUENCODE_USE_GPU_ACCELERATION=true
VUENCODE_GPU_DEVICE_ID=0
