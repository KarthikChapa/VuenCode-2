# === Caching (Aggressive) ===
VUENCODE_ENABLE_CACHING=true
VUENCODE_CACHE_TTL_SECONDS=3600
VUENCODE_REDIS_URL=redis://localhost:6379

# === Monitoring (Production) ===
VUENCODE_ENABLE_METRICS=true
VUENCODE_LOG_LEVEL=INFO

# === Fallback System (Robust) ===
VUENCODE_ENABLE_FALLBACK=true
VUENCODE_FALLBACK_RESPONSE_CACHE=true
VUENCODE_EMERGENCY_MODE_THRESHOLD=0.95

# === Deployment Tools ===
VUENCODE_NGROK_TOKEN=your_ngrok_token_here

# === Competition Mode Flag ===
COMPETITION_MODE=true
