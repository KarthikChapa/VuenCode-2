# VuenCode Local Development Configuration
# Optimized for CPU-based development and fast iteration

# === Deployment Settings ===
VUENCODE_DEPLOYMENT_MODE=local
VUENCODE_PERFORMANCE_PROFILE=development

# === API Configuration ===
VUENCODE_API_HOST=127.0.0.1
VUENCODE_API_PORT=8000
VUENCODE_API_WORKERS=1

# === Model Configuration ===
# Leave Gemini API key empty for local stub mode
VUENCODE_GEMINI_API_KEY=AIzaSyA4LCrRRlopK-lFxPNBq6WSMFQ3_PacDc8
VUENCODE_GEMINI_FLASH_MODEL=gemini-2.5-flash
VUENCODE_GEMINI_PRO_MODEL=gemini-2.5-pro

# === Performance Settings ===
VUENCODE_TARGET_LATENCY_MS=200
VUENCODE_MAX_CONCURRENT_REQUESTS=5

# === Video Processing ===
VUENCODE_MAX_FRAMES_PER_VIDEO=8
VUENCODE_DEFAULT_FPS_SAMPLE=0.5
VUENCODE_SCENE_DETECTION_THRESHOLD=0.3

# === GPU Settings (Disabled for local) ===
VUENCODE_USE_GPU_ACCELERATION=false
VUENCODE_TENSORRT_OPTIMIZATION=false

# === Caching ===
VUENCODE_ENABLE_CACHING=true
VUENCODE_CACHE_TTL_SECONDS=1800

# === Monitoring ===
VUENCODE_ENABLE_METRICS=true
VUENCODE_LOG_LEVEL=DEBUG

# === Development Tools ===
VUENCODE_ENABLE_FALLBACK=true

# === Phase 2 Enhanced Features ===
VUENCODE_ENABLE_VST_COMPRESSION=true
VUENCODE_ENABLE_MULTIMODAL_FUSION=true
VUENCODE_ENABLE_AUDIO_PROCESSING=true
VUENCODE_WHISPER_MODEL_SIZE=base
VUENCODE_VST_MAX_TOKENS_PER_MINUTE=15
VUENCODE_MULTIMODAL_EMBEDDING_DIM=768